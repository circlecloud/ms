{
    "compilerOptions": {
        "baseUrl": "src",
        "outDir": "dist",
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "strictNullChecks": false,
        "sourceMap": false,
        "declaration": true,
        "declarationMap": true,
        "noImplicitAny": false,
        "downlevelIteration": true,
        "allowUnreachableCode": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": []
    }
}